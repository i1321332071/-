}
/* mask2 */
.mask-firstlist{
   height: 550rpx;
  overflow: hidden;
}
.mask-title{
color: #4e4b4b;
padding: 15rpx 15rpx;
}
.mask-box{
  display: inline-block;
  width: 32%;
  color: #6A6A6A;
  text-align: center;
  border-radius: 5rpx;
  background-color: rgb(250, 245, 245);
  margin: 10rpx 5rpx;
  padding: 10rpx 0;
}
.mask-boxselected{
  background: #FFFBF1;
  border-color: #FFE9B7;
  color: rgb(231, 194, 119);
}
.mask-footer{
   height: 90rpx;
  display: flex;
  border-top: 1rpx solid #ECECEC;
}
.mask-footbt1{
  flex: 1;
  align-items: center;
  text-align: center;
  font-size: 30rpx;
  line-height: 90rpx;
  padding-left: 30rpx;
}
.mask-icon{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  background-color: red;
  color: white;
  border-radius:50%;
text-align:center;
right:48%;
z-index:99999;

}
.mask-footbt2{
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  line-height: 90rpx;
  font-size: 30rpx;
  width: 50%;
  background: #FFD161; 
  position: relative;
  
}
.sort-list{
