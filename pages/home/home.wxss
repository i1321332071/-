.offline{
  position: absolute;
  height: 100%;
  width: 100%;
  z-index: 99999999999999999;
  background-color: #fcfcfc;
  text-align:center;
vertical-align:middle;
padding-top:250px;
}
.offline-bt{
  width:30%;
font-size:28rpx;
background-color: #FFD161;
color: white;
margin-top: 50px;
}
.mask{
  width:100%;
  height:100%;
  position: fixed;
  z-index:999;
  background-color:rgba(15, 15, 26, 0.7);
}
.mask_{
  position: fixed;
  z-index:9999;
}
.masklist{
background-color: white;
font-size:25rpx;

}
/* mask1 */
